{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions":{
    "baseUrl": "./src",
    "paths": {
      "@components": ["./components"],
      "@components/*": ["./components/*"],

      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"],

      "@constants": ["./constants"],
      "@constants/*": ["./constants/*"],

      "@elements": ["./elements"],
      "@elements/*": ["./elements/*"],

      "@modules": ["./modules"],
      "@modules/*": ["./modules/*"],

      "@pages": ["./pages"],
      "@pages/*": ["./pages/*"],

      "@services": ["./services"],
      "@services/*": ["./services/*"],

      "@state": ["./state"],
      "@state/*": ["./state/*"],

      "@customTypes": ["./customTypes"],
      "@customTypes/*": ["./customTypes/*"],

      "@router": ["src/router"],
      "@router/*": ["src/router/*"],

      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
